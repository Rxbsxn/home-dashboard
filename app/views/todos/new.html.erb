<%= stylesheet_link_tag 'todos' %>
  <link href="https://fonts.googleapis.com/css?family=Dosis" rel="stylesheet">


  <div id="formtodo">
<%= form_for @todo do |f| %>
<h2>  <%= f.label :title %><br>
  <%= f.text_field :title %>
</h2>
<h2>  <%= f.label :content %><br>
  <%= f.text_field :content %>
</h2>
<h2>
  <%= image_submit_tag("blackarrow.png", :width => "30")%>
</h2>
<% end %>
</div>
